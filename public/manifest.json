{

  "description": "Copies the name of the branch to generate directly into clipboard",
  "manifest_version": 2,
  "name": "GitAndJira",
  "version": "--VERSION--",
  "homepage_url": "https://b2f.ddnss.org/",
  "icons": {
    "48": "icons/48.png"
  },

  "permissions": [
    "activeTab",
    "clipboardWrite"
  ],

  "browser_specific_settings": {
    "gecko": {
      "id": "GitAndJira@b2f.ddnss.org"
    }
  },

  "browser_action": {
    "default_icon": "icons/32.png",
    "default_title": "BranchJira",
    "default_popup": "popup.html"
  }
}
